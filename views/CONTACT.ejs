    <main>

        <div class="buttons">
            <a href="https://www.facebook.com" class="button facebook">Facebook</a>
            <a href="https://www.twitter.com" class="button twitter">Twitter</a>
            <a href="https://www.linkedin.com" class="button linkedin">LinkedIn</a>
            <a href="https://www.instagram.com" class="button instagram">Instagram</a>
            <a href="https://www.gmail.com" class="button contact">Email</a>
          </div>
        </main>
        
        <style>
          main {
            max-width: 800px;
            margin: 0 auto;
            padding: 20px;
            background-color: #f5f5f5;
            border-radius: 10px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
          }
        
          p {
            font-size: 18px;
            line-height: 1.5;
            margin-bottom: 20px;
          }
        
          p:last-child {
            margin-bottom: 0;
          }
        
          p:first-child {
            margin-top: 0;
          }
        
          h1 {
            font-size: 32px;
            margin-bottom: 30px;
          }
        
          .buttons {
            display: flex;
            justify-content: center;
            margin-top: 20px;
          }
        
          .button {
            display: inline-block;
            margin-right: 10px;
            padding: 10px 20px;
            font-size: 16px;
            font-weight: bold;
            text-transform: uppercase;
            color: #fff;
            border-radius: 5px;
            box-shadow: 0 0 5px rgba(0, 0, 0, 0.2);
            transition: all 0.3s ease;
          }
        
          .button:hover {
            transform: translateY(-2px);
            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);
          }
        
          .facebook {
            background-color: #3b5998;
          }
        
          .twitter {
            background-color: #1da1f2;
          }
        
          .linkedin {
            background-color: #0077b5;
          }
        
          .instagram {
            background-color: #c13584;
          }
        
          .contact {
            background-color: #008cba;
          }
          footer {
			background-color: #333;
			color: #fff;
			text-align: center;
			padding: 20px;
			position: fixed;
			bottom: 0;
			left: 0;
			right: 0;
		}
        </style>
<footer>
